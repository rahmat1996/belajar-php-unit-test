{
    "name": "rahmat1996/belajar-php-unit-test",
    "description": "Belajar PHP Unit Test",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Rahmat1996\\Test\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rahmat1996\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Rahmat",
            "email": "rahmat14121996@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.3"
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml",
        "test:product": "phpunit --configuration phpunit.xml --testsuite product",
        "test:counter": "phpunit --configuration phpunit.xml --testsuite counter"
    }
}